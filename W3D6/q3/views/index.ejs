<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Q3</title>
</head>
<body>
    <h1>Products</h1>
    <ul>
        <% products.forEach(product => { %>
            <li>
                <a href="/product/<%= product.id %>"><%= product.name %></a> - $<%= product.price %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
